{
  "name": "eslint-config-availity",
  "version": "4.0.8",
  "description": "Shareable ESLint config for Availity projects",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "jest .",
    "release": "npm run lint && sh ./scripts/release.sh",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "engines": {
    "node": ">=8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Availity/eslint-config-availity.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "availity"
  ],
  "files": [
    "index.js",
    "browser.js",
    "ts-recommended.js",
    "base.js",
    "README.md"
  ],
  "author": "Robert McGuinness",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Availity/eslint-config-availity/issues"
  },
  "homepage": "https://github.com/Availity/eslint-config-availity#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.2.0",
    "eslint-plugin-unicorn": "^9.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "conventional-changelog-cli": "^2.0.21",
    "conventional-recommended-bump": "^5.0.0",
    "husky": "^2.4.1",
    "is-plain-obj": "^2.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.0.1",
    "lodash.find": "^4.6.0",
    "prettier": "^1.18.2",
    "temp-write": "^4.0.0",
    "typescript": "^3.5.2"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
