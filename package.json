{
  "name": "eslint-config-availity",
  "version": "3.0.0-beta.10",
  "description": "Shareable ESLint config for Availity projects",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "tape test.js",
    "release": "npm run lint && sh ./scripts/release.sh",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "engines": {
    "node": ">=8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Availity/eslint-config-availity.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "availity"
  ],
  "files": [
    "index.js",
    "browser.js",
    "base.js",
    "README.md"
  ],
  "author": "Robert McGuinness",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Availity/eslint-config-availity/issues"
  },
  "homepage": "https://github.com/Availity/eslint-config-availity#readme",
  "peerDependencies": {
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-config-prettier": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^5.2.5",
    "@commitlint/config-conventional": "^5.2.3",
    "@commitlint/config-lerna-scopes": "^5.2.0",
    "babel-eslint": "^8.0.1",
    "conventional-changelog-cli": "^1.3.4",
    "conventional-recommended-bump": "^1.0.2",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.0",
    "husky": "^0.14.3",
    "is-plain-obj": "1.1.0",
    "lint-staged": "^6.0.0",
    "lodash.find": "^4.6.0",
    "prettier": "^1.9.1",
    "tape": "^4.6.3",
    "temp-write": "^3.3.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional",
      "@commitlint/config-lerna-scopes"
    ]
  }
}
